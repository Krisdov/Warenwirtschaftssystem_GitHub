<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="SWEP Checkstyle Config" location="swep_checkstyle_v4.2.xml" type="project" description="RWU WI SWEP specific configuration check-style">
    <property name="checkstyle.cache.file" value="${project_loc}/target/checkstyle-cachefile"/>
    <property name="checkstyle.suppressions.file" value="${project_loc}/swep_suppressions.xml"/>
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="SWEP Checkstyle Config" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
</fileset-config>
